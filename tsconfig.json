{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "skipLibCheck": true,

    "moduleResolution": "NodeNext",
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "outDir": "dist",

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "pretty": true
  },
  "exclude": ["src/generated/client"]
}
